    "@types/jest": "^25.1.5",
    "@types/react": "^16.9.31",
    "@types/react-dom": "^16.9.6",    
    "autoprefixer": "^9.7.5",
    "concurrently": "5.1.0",
    "jest": "^25.2.7",
    "parcel": "^1.12.4",
    "postcss-modules": "^1.5.0",
    "react-hot-loader": "^4.12.20",
    "sass": "^1.26.3",
    "ts-jest": "^25.3.1",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3",
    "typescript-plugin-css-modules": "^2.2.0",
